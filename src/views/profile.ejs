<!DOCTYPE html>
<html>
  <%- include('partials/head') %>

  <body>
    <%- include('partials/header') %> <%- include('partials/nav') %>

    <main>
      <h1><%= user.name %>'s Profile</h1>
      <h2>Hello <%= user.firstname %>,</h2>
      <p>ID: <%= user.id %></p>
      <h2>Hobbies</h2>
      <ul>
        <% user.key.forEach(function(item) { %>
        <li><%= item %></li>
        <% }); %>
      </ul>
    </main>

    <%- include('partials/footer') %>
  </body>
</html>
